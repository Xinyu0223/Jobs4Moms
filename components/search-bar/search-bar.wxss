/* components/search-bar/search-bar.wxss */
@import '/styles/weui.wxss';

.searchbar{
  background-color: #f7f6f6;
  padding: 15rpx;
}
.search-navigator{
  width: 100%;
  height: 10px;  
  border-radius:10rpx;
  background-color: #ffffff;
  background-position: center center;
  background-repeat: no-repeat;
  background-size: 8%;  
}
.search-input-group{
  width: 100%;
  height: 60rpx;
  background-color: #fff;
  border-radius: 10rpx;
  padding: 5rpx 10rpx;
  box-sizing: border-box;
}
.search-input{
  min-height: 40rpx;
  height: 40rpx;
  font-size: 14px;
}


